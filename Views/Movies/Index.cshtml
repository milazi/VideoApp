@model List<Vidly.Models.Movie>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>
<table class="table table-striped">
    <tr>
        <th>Movies</th>
    </tr>
    @foreach(var movie in Model)
    {
        <tr>
            <td> @movie.Name </td>
        </tr>
    }
</table>


